{
    "env": {
        "node": true
    },
    "rules": {
        "quotes": [2, "single"],
        "strict": 2,
        "no-process-exit": 2,
        "eqeqeq": 2,
        "semi": [2, "always"],
        "no-mixed-requires": 0,
        "valid-jsdoc": 0,
        "comma-dangle": ["error", "never"],
        "no-trailing-spaces": 2
    }
}